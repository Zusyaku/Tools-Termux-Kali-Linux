mainmenu () {
	clear
	f_un_ban && f_un_show
	echo -ne $okegreen"\t"$red"<"$white"{"$okegreen"0x25/Choice"$white"}"$red"> " $RESET
	read mainchoice
	echo
	case $mainchoice in 
	1) f_op_1 ;;
	2) f_op_2 ;;
	3) f_op_3 ;;
	4) f_op_4 ;;
	5) f_op_5 ;;
	6|Q|q|exit|quit) exit ;;
	*) mainmenu ;;
	esac

}

clear
mainmenu

